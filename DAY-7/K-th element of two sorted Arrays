class Solution {
    public long kthElement( int arr1[], int arr2[], int n, int m, int k) {
     int result[]=new int[n+m];
     for(int i=0;i<n;i++)
     {
         result[i]=arr1[i];
     }
     
     for(int i=0;i<m;i++)
     {
         result[n+i]=arr2[i];
     }
     Arrays.sort(result);
     return result[k-1];
        
    }
}
